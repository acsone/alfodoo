<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-name="cmis_web.UpdateDocumentContentDialog" owl="1">
        <Dialog size="'md'" title="props.title">
            <input type="file" class="form-control" t-ref="fileInput" />
            <t t-set-slot="footer" owl="1">
                <button
                    class="btn btn-primary"
                    t-on-click="onConfirm"
                >Update content</button>
                <button class="btn btn-secondary" t-on-click="onClose">Close</button>
            </t>
        </Dialog>
    </t>
</templates>
