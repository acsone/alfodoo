<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="cmis_web.CmisActions" owl="1">
        <button type="button" title="Download" class="btn btn-outline-primary" t-on-click="onClickDownload">
            <i class="fa fa-download"/>
        </button>
        <button type="button" title="Preview" class="btn btn-outline-primary" t-on-click="onClickPreview">
            <i class="fa fa-eye"/>
        </button>
        <button type="button" title="More actions" class="btn btn-outline-primary" t-on-click="(ev) => ev.stopPropagation()">
            <i class="fa fa-align-justify"/>
            <Dropdown toggler="'parent'">
                <DropdownItem>
                    <div t-on-click="onDelete">Delete</div>
                </DropdownItem>
            </Dropdown>
        </button>
    </t>
</templates>
