<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="cmis_web.AddDocumentDialog" owl="1">
        <Dialog size="'md'" title="'Add Document'">
            <input type="file" class="form-control" multiple="true" t-ref="filesInput" />
            <t t-set-slot="footer" owl="1">
                <button class="btn btn-primary" t-on-click="onConfirm">Add</button>
                <button class="btn btn-secondary" t-on-click="onClose">Close</button>
            </t>
        </Dialog>
    </t>
</templates>
